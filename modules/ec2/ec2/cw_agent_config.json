{
    "agent": {
        "metrics_collection_interval": 60
    },
    "metrics": {
        "namespace": "CWAgent",
        "aggregation_dimensions": [
            [
                "InstanceId",
                "InstanceType"
            ],
            [
                "InstanceId"
            ]
        ],
        "metrics_collected": {
            "mem": {
                "measurement": [
                    "mem_used_percent",
                    "mem_available_percent"
                ]
            },
            "swap": {
                "measurement": [
                    "swap_used_percent"
                ]
            },
            "disk": {
                "ignore_file_system_types": [
                    "sysfs",
                    "devtmpfs"
                ],
                "measurement": [
                    "disk_used_percent"
                ],
                "resources": [
                    "*"
                ]
            }
        }
    }
}